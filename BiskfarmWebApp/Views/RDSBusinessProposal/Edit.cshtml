@using Biskfarm.DAL.Model
@{
    Layout = "~/Views/Shared/_LayoutForms.cshtml";
}

@model RDS_BUSINESS_PROPOSAL

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>RDS Business Proposal</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">RDS Business Proposal</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<section class="content">
    <div class="container-fluid">
        <form id="business">
            <div class="row">
                <div class="col-md-6">
                    <div class="card-body">

                        <div class="form-group">
                            <label>Principal Co. </label>
                            <input type="text" required class="form-control" name="principalCo" placeholder="Principal Company" id="principalCo" value="@Model.principalCo">
                        </div>

                    </div>

                </div>
                <div class="col-md-6">
                    <div class="card-body">

                        <div class="form-group">
                            <label>Date:</label>
                            <input type="text" class="form-control" name="dateOfEntry" id="dateOfEntry" data-inputmask-alias="datetime" data-inputmask-inputformat="dd/mm/yyyy" data-mask value="@Model.dateOfEntry">
                        </div>

                    </div>

                </div>
            </div>

            <div class="row">
                <div class="col-md-6">
                    <div class="card-body">

                        <div class="form-group">
                            <label>Name of RDS / Super Stockist </label>
                            <input type="text" required class="form-control" name="nameOfRDSSuperStockist" placeholder="Name of RDS / Super Stockist" id="nameOfRDSSuperStockist" value="@Model.nameOfRDSSuperStockist">
                        </div>

                    </div>

                </div>
                <div class="col-md-6">
                    <div class="card-body">

                        <div class="form-group">
                            <label for="hori-pass1" class="col-4 col-form-label">Appointment Plan </label>
                            <!-- text input -->
                            <div class="form-group">
                                <select name="appointmentPlanId" id="appointmentPlanId" class="form-control" placeholder="Select">
                                    @if (Model.appointmentPlanId == 1)
                                    {
                                        <option value="1" selected>Replacement</option>
                                        <option value="2">New Appointment</option>
                                    }
                                    else
                                    {
                                        <option value="1" >Replacement</option>
                                        <option value="2" selected>New Appointment</option>
                                    }

                                </select>
                            </div>
                        </div>

                    </div>

                </div>
            </div>

            <div class="row">
                <div class="col-md-4">
                    <div class="card-body">

                        <div class="form-group">
                            <label>Town </label>
                            <input type="text" required class="form-control" name="town" placeholder="Town" id="town" value="@Model.town">
                        </div>

                    </div>

                </div>
                <div class="col-md-4">
                    <div class="card-body">

                        <div class="form-group">
                            <label>District </label>
                            <input type="text" required class="form-control" name="district" placeholder="District" id="district" value="@Model.district">
                        </div>

                    </div>

                </div>
                <div class="col-md-4">
                    <div class="card-body">

                        <div class="form-group">
                            <label>State </label>
                            <input type="text" required class="form-control" name="state" placeholder="State" id="state" value="@Model.state">
                        </div>

                    </div>

                </div>
            </div>

            <div class="container-fluid">
                <div class="row mb-2">
                    <div class="col-sm-6">
                        <h1>Part-A</h1>
                    </div>

                </div>
            </div>


            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Distributor Management factor</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body table-responsive p-0">
                    <table class="table table-hover text-nowrap table-striped">
                        <thead>
                            <tr>
                                <th></th>
                                <th>General Retail</th>
                                <th>Key Outlet</th>
                                <th>Wholesale</th>
                                <th>Other</th>
                                <th>Total</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Sales (Rs/month)</td>
                                <td><input type="number" min="0" step="0.01" id="salesGenRetail" name="salesGenRetail" value="@Model.salesGenRetail"/></td>
                                <td><input type="number" min="0" step="0.01" id="salesKeyOutlet" name="salesKeyOutlet" value="@Model.salesKeyOutlet" /></td>
                                <td><input type="number" min="0" step="0.01" id="salesWholesale" name="salesWholesale" value="@Model.salesWholesale" /></td>
                                <td><input type="number" min="0" step="0.01" id="salesOther" name="salesOther" value="@Model.salesOther" /></td>
                                <td><input type="number" min="0" step="0.01" id="salesTot" readonly /></td>
                            </tr>
                            <tr>
                                <td>Stock day Norm (Mutual Agreement)</td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td><input type="number" min="0" step="0.01" id="stockDayNormTotal" name="stockDayNormTotal" /></td>
                            </tr>
                            <tr>
                                <td>Stock Value Norm (Mutual Agreement)</td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td><input type="number" min="0" step="0.01" id="stockValueNormTotal" readonly name="stockValueNormTotal" /></td>
                            </tr>
                            <tr>
                                <td>Market Disc Category</td>
                                <td><input type="number" min="0" step="0.01" id="marketDiscCatGenRetail" />%</td>
                                <td><input type="number" min="0" step="0.01" id="marketDiscCatKeyOutlet" />%</td>
                                <td><input type="number" min="0" step="0.01" id="marketDiscCatWholesale" />%</td>
                                <td><input type="number" min="0" step="0.01" id="marketDiscCatOther" />%</td>
                                <td></td>
                            </tr>
                            <tr>
                                <td>Outlets (by disc. Category)</td>
                                <td><input type="number" min="0" step="0.01" id="outletsGR" name="outletsGR" /></td>
                                <td><input type="number" min="0" step="0.01" id="outletsKO" name="outletsKO" /></td>
                                <td><input type="number" min="0" step="0.01" id="outletsW" name="outletsW" /></td>
                                <td><input type="number" min="0" step="0.01" id="outletsO" name="outletsO" /></td>
                                <td><input type="number" min="0" step="0.01" id="outletsTot" /></td>
                            </tr>
                            <tr>
                                <td>Credit day Norm (Mutual Agreement)</td>
                                <td><input type="number" min="0" step="0.01" id="cdNormGR" name="cdNormGR" /></td>
                                <td><input type="number" min="0" step="0.01" id="cdNormKO" name="cdNormKO" /></td>
                                <td><input type="number" min="0" step="0.01" id="cdNormW" name="cdNormW" /></td>
                                <td><input type="number" min="0" step="0.01" id="cdNormO" name="cdNormO" /></td>
                                <td><input type="number" min="0" step="0.01" id="cdNormTot" /></td>
                            </tr>
                            <tr>
                                <td>Credit Value Norm (Mutual Agreement)</td>
                                <td><input type="number" min="0" step="0.01" id="cvNormGR" name="cvNormGR" /></td>
                                <td><input type="number" min="0" step="0.01" id="cvNormKO" name="cvNormKO" /></td>
                                <td><input type="number" min="0" step="0.01" id="cvNormW" name="cvNormW" /></td>
                                <td><input type="number" min="0" step="0.01" id="cvNormO" name="cvNormO" /></td>
                                <td><input type="number" min="0" step="0.01" id="cvNormTot" /></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!-- /.card-body -->
            </div>


            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Sales of Other companies and firm's total turnover</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body table-responsive p-0">
                    <table class="table table-hover text-nowrap table-striped">
                        <thead>
                            <tr>
                                <th></th>
                                <th>Company 1</th>
                                <th>Company 2</th>
                                <th>Other Company (in total)</th>
                                <th>Total</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Rs/month</td>
                                <td><input type="number" min="0" step="0.01" id="com1Tot" name="com1Tot" /></td>
                                <td><input type="number" min="0" step="0.01" id="com2Tot" name="com2Tot" /></td>
                                <td><input type="number" min="0" step="0.01" id="othersTot" name="othersTot" /></td>
                                <td><input type="number" min="0" step="0.01" id="comTot" /></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!-- /.card-body -->
            </div>


            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Order Route Plan</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body table-responsive p-0">
                    <table class="table table-hover text-nowrap table-striped">
                        <thead>
                            <tr>
                                <th>Frequency</th>
                                <th>Weekly</th>
                                <th>Fortnightly</th>
                                <th>Total</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>No. of Routes</td>
                                <td><input type="number" min="0" id="routesWeekly" name="routesWeekly" /></td>
                                <td><input type="number" min="0" id="routesFortNight" name="routesFortNight" /></td>
                                <td><input type="number" min="0" id="routesTot" /></td>
                            </tr>
                            <tr>
                                <td>No. of Outlets</td>
                                <td><input type="number" min="0" id="outletsWeekly" name="outletsWeekly" /></td>
                                <td><input type="number" min="0" id="outletsFortNight" name="outletsFortNight" /></td>
                                <td><input type="number" min="0" id="outletsTotal" /></td>
                            </tr>
                            <tr>
                                <td>No. of Salesman</td>
                                <td><input type="number" min="0" id="salesWeekly" name="salesWeekly" /></td>
                                <td><input type="number" min="0" id="salesFortNight" name="salesFortNight" /></td>
                                <td><input type="number" min="0" id="salesTotal" /></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!-- /.card-body -->
            </div>


            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Condition 1 (Business Structure)</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body table-responsive p-0">
                    <table class="table table-hover text-nowrap table-striped">
                        <thead>
                            <tr>
                                <th></th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Our Share of Business of total stockist turnover</td>
                                <td><input type="number" min="0" step="0.01" readonly id="ourShareOfBusiness" name="ourShareOfBusiness" />%</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!-- /.card-body -->
            </div>


            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Condition 2 (Infrastructure and redistribution Cost)</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body table-responsive p-0">
                    <table class="table table-hover text-nowrap table-striped">
                        <thead>
                            <tr>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Number of Units Required (Supply)</td>
                                <td># Units</td>
                                <td><input type="number" min="0" step="0.01" id="nofUnits" name="nofUnits" /></td>
                                <td>Automated</td>
                                <td><input type="number" min="0" step="0.01" id="nofAutomated" name="nofAutomated" /></td>
                                <td>Manual</td>
                                <td><input type="number" min="0" step="0.01" id="nofManual" name="nofManual" /></td>
                            </tr>
                            <tr>
                                <td>Number of Units Required (Supply)</td>
                                <td>Exclusive</td>
                                <td><input type="number" min="0" step="0.01" id="nofExclusive" name="nofExclusive" /></td>
                                <td>Shared</td>
                                <td><input type="number" min="0" step="0.01" id="nofShared" name="nofShared" /></td>
                                <td></td>
                                <td></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!-- /.card-body -->


                <div class="card-body table-responsive p-0">
                    <table class="table table-hover text-nowrap table-striped">
                        <thead>
                            <tr>
                                <th>Redistribution Expense</th>
                                <th>AUTOMATED UNIT (If any)</th>
                                <th>MANUAL UNIT (If any)</th>
                                <th>Unit Cost</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Unit Share % by Bisk Farm</td>
                                <td><input type="number" min="0" step="0.01" id="unitShareAutomated" name="unitShareAutomated" />%</td>
                                <td><input type="number" min="0" step="0.01" id="unitShareManual" name="unitShareManual" />%</td>
                                <td></td>
                            </tr>
                            <tr>
                                <td>DRIVER</td>
                                <td><input type="number" min="0" step="0.01" id="driverAutomated" name="driverAutomated" /></td>
                                <td><input type="number" min="0" step="0.01" id="driverManual" name="driverManual" /></td>
                                <td></td>
                            </tr>
                            <tr>
                                <td>HELPER</td>
                                <td><input type="number" min="0" step="0.01" id="helperAutomated" name="helperAutomated" /></td>
                                <td><input type="number" min="0" step="0.01" id="helperManual" name="helperManual" /></td>
                                <td></td>
                            </tr>
                            <tr>
                                <td>Unit running Cost (Fuel and Maintainance)</td>
                                <td><input type="number" min="0" step="0.01" id="unitRunningCostAutomated" name="unitRunningCostAutomated" /></td>
                                <td><input type="number" min="0" step="0.01" id="unitRunningCostManual" name="unitRunningCostManual" /></td>
                                <td></td>
                            </tr>
                            <tr>
                                <td>Total Cost (Redistribution)</td>
                                <td><input type="number" min="0" step="0.01" id="totRedAuto" name="totRedAuto" readonly /></td>
                                <td><input type="number" min="0" step="0.01" id="totRedManual" name="totRedManual" readonly /></td>
                                <td><input type="number" min="0" step="0.01" id="totUnit" name="totUnit" readonly /></td>
                            </tr>
                        </tbody>
                    </table>
                </div>


                <div class="card-body table-responsive p-0">
                    <table class="table table-hover text-nowrap table-striped">
                        <thead>
                            <tr>
                                <th>Salesman Expense</th>
                                <th>Salary</th>
                                <th>TA / DA (If any)</th>
                                <th>DS Cost</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Total Cost (Sales Man)</td>
                                <td><input type="number" min="0" step="0.01" id="salesmanSalary" name="salesmanSalary" /></td>
                                <td><input type="number" min="0" step="0.01" id="salesmanTA" name="salesmanTA" /></td>
                                <td><input type="number" min="0" step="0.01" id="dsCost" readonly /></td>
                            </tr>
                        </tbody>
                    </table>
                </div>

                <div class="card-body table-responsive p-0">
                    <table class="table table-hover text-nowrap table-striped">
                        <thead>
                            <tr>
                                <th>Condition 3 (Indirect Expenditure)</th>
                                <th>Amount</th>
                                <th>Indirect Cost</th>
                                <th>Remarks (If any)</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Godown Expenses </td>
                                <td><input type="number" min="0" step="0.01" id="godownAmount" name="godownAmount" /></td>
                                <td></td>
                                <td><input type="text" id="godownRemarks" name="godownRemarks" /></td>
                            </tr>
                            <tr>
                                <td>Computer Operator  </td>
                                <td><input type="number" min="0" step="0.01" id="computerAmount" name="computerAmount" /></td>
                                <td></td>
                                <td><input type="text" id="computerRemarks" name="computerRemarks" /></td>
                            </tr>
                            <tr>
                                <td>Office expenses (Incl Stationery) </td>
                                <td><input type="number" min="0" step="0.01" id="officeAmount" name="officeAmount" /></td>
                                <td></td>
                                <td><input type="text" id="officeRemarks" name="officeRemarks" /></td>
                            </tr>
                            <tr>
                                <td>Overhead Expense</td>
                                <td><input type="number" min="0" step="0.01" id="overheadAmount" name="overheadAmount" /></td>
                                <td></td>
                                <td><input type="text" id="overheadRemarks" name="overheadRemarks" /></td>
                            </tr>
                            <tr>
                                <td>Accounting and Legal Expenses</td>
                                <td><input type="number" min="0" step="0.01" id="accountingAmount" name="accountingAmount" /></td>
                                <td></td>
                                <td><input type="text" id="accountingRemarks" name="accountingRemarks" /></td>
                            </tr>
                            <tr>
                                <td>Sundry Expenses</td>
                                <td><input type="number" min="0" step="0.01" id="sundryAmount" name="sundryAmount" /></td>
                                <td></td>
                                <td><input type="text" id="sundryRemarks" name="sundryRemarks" /></td>
                            </tr>
                            <tr>
                                <td><h3>Total Expenditure</h3></td>
                                <td><input type="number" min="0" step="0.01" id="totExpenditure" /></td>
                                <td><input type="number" min="0" step="0.01" id="indirectCost" /></td>
                                <td><input type="text" /></td>
                            </tr>
                        </tbody>
                    </table>
                </div>


            </div>




            <div class="card">
                @*  <div class="card-header">
                <h3 class="card-title">Condition 1 (Business Structure)</h3>
                </div> *@
                <!-- /.card-header -->
                <div class="card-body table-responsive p-0">
                    <table class="table table-hover text-nowrap table-striped">
                        <thead>
                            <tr>
                                <th>INVESTMENT CALCULATIONS</th>
                                <th>Calculation</th>
                                <th>Unit</th>
                                <th>Value</th>
                                <th>No. of Days</th>
                                <th>Norm</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Stock Investment</td>
                                <td>(Stock/Sales) x No of days </td>
                                <td>Rs</td>
                                <td><input type="number" min="0" step="0.01" id="siVal" name="siVal" readonly /></td>
                                <td><input type="number" min="0" step="0.01" id="siDays" name="siDays" readonly /></td>
                                <td><input type="number" min="0" step="0.01" id="siNorm" name="siNorm" /></td>
                            </tr>
                            <tr>
                                <td>Market outstanding</td>
                                <td>(Mkt.Credit/Sales) x No of days </td>
                                <td>Rs</td>
                                <td><input type="number" min="0" step="0.01" id="moVal" name="moVal" readonly /></td>
                                <td><input type="number" min="0" step="0.01" id="moDays" name="moDays" readonly /></td>
                                <td><input type="number" min="0" step="0.01" id="moNorm" name="moNorm" /></td>
                            </tr>
                            <tr>
                                <td>Claims</td>
                                <td>(Value Claim/Sales) x No of days </td>
                                <td>Rs</td>
                                <td><input type="number" min="0" step="0.01" id="claimsVal" name="claimsVal" /></td>
                                <td><input type="number" min="0" step="0.01" id="claimsDays" name="claimsDays" /></td>
                                <td><input type="number" min="0" step="0.01" id="claimsNorm" name="claimsNorm" /></td>
                            </tr>
                            <tr>
                                <td>Gross Investment</td>
                                <td></td>
                                <td>Rs</td>
                                <td><input type="number" min="0" step="0.01" id="giVal" /></td>
                                <td><input type="number" min="0" step="0.01" id="giDays" /></td>
                                <td><input type="number" min="0" step="0.01" id="giNorm" /></td>
                            </tr>
                            <tr>
                                <td>Company credit</td>
                                <td></td>
                                <td>Rs</td>
                                <td><input type="number" min="0" step="0.01" id="comCreditVal" name="comCreditVal" /></td>
                                <td></td>
                                <td></td>
                            </tr>
                            <tr>
                                <td>Net Investment</td>
                                <td></td>
                                <td>Rs</td>
                                <td><input type="number" min="0" step="0.01" id="niVal" /></td>
                                <td></td>
                                <td></td>
                            </tr>
                            <tr>
                                <td>Annual rotations</td>
                                <td></td>
                                <td>Number</td>
                                <td><input type="number" min="0" step="0.01" id="arVal" /></td>
                                <td></td>
                                <td></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!-- /.card-body -->
            </div>

            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">RDS BUSINESS PROPOSAL FORM</h3>
                </div>
                <div class="card-header">
                    <h3 class="card-title">PART - B (Page 2)</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body table-responsive p-0">
                    <table class="table table-hover text-nowrap table-striped">
                        <thead>
                            <tr>
                                <th>NET MARGIN CALCULATIONS</th>
                                <th></th>
                                <th>Calculation</th>
                                <th>Unit</th>
                                <th>Output</th>
                                <th>Remarks</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Total Monthly Sales</td>
                                <td></td>
                                <td>a=b+c+d </td>
                                <td>Rs/month</td>
                                <td><input type="number" min="0" step="0.01" id="tmsOut" readonly /></td>
                                <td><input type="text" id="tmsRemarks" name="tmsRemarks" /></td>
                            </tr>
                            <tr>
                                <td>Margin - All Products given by Company </td>
                                <td></td>
                                <td>i</td>
                                <td>%</td>
                                <td><input type="text" id="marginPercent" name="marginPercent" /></td>
                                <td><input type="text" id="marginRemarks" name="marginRemarks" /></td>
                            </tr>
                            <tr>
                                <td>Gross Margin</td>
                                <td></td>
                                <td>j=I x a </td>
