                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="col-sm-12">
                                            <label>Area of Operation:</label>
                                            <input type="text" required class="form-control" name="areaOfOp" placeholder="Area Of Operation" id="areaOfOp" value="@Model.areaOfOp">
                                        </div>
                                    </div>
                                </div>

