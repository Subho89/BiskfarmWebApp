                                    <div class="col-sm-4">
                                        <div class="col-sm-12">
                                            <label>Value Per Order: </label>
                                            <input type="text" required class="form-control" name="valuePerOrder" placeholder="Value Per Order" id="valuePerOrder" value="@Model.valuePerOrder">
